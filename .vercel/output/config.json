{
  "version": 3,
  "overrides": {
    "index.html": {
      "path": ""
    },
    "about/index.html": {
      "path": "about"
    },
    "categories/index.html": {
      "path": "categories"
    },
    "contact/index.html": {
      "path": "contact"
    },
    "featured/index.html": {
      "path": "featured"
    },
    "auth/logout/index.html": {
      "path": "auth/logout"
    },
    "blogs/index.html": {
      "path": "blogs"
    },
    "support/index.html": {
      "path": "support"
    },
    "blogs/ex-2/index.html": {
      "path": "blogs/ex-2"
    },
    "auth/signup/index.html": {
      "path": "auth/signup"
    },
    "auth/login/index.html": {
      "path": "auth/login"
    },
    "blogs/pinia/index.html": {
      "path": "blogs/pinia"
    },
    "blogs/example/index.html": {
      "path": "blogs/example"
    }
  },
  "routes": [
    {
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "src": "/_nuxt/builds/meta/(.*)"
    },
    {
      "headers": {
        "Content-Type": "application/xslt+xml"
      },
      "src": "/__sitemap__/style.xsl"
    },
    {
      "headers": {
        "cache-control": "public, max-age=1, immutable"
      },
      "src": "/_nuxt/builds/(.*)"
    },
    {
      "headers": {
        "Content-Type": "text/xml; charset=UTF-8",
        "Cache-Control": "public, max-age=600, must-revalidate",
        "X-Sitemap-Prerendered": "2025-05-24T09:22:01.345Z"
      },
      "src": "/sitemap.xml"
    },
    {
      "headers": {
        "X-Robots-Tag": "noindex"
      },
      "src": "/_nuxt"
    },
    {
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "X-Robots-Tag": "noindex"
      },
      "src": "/_nuxt/(.*)"
    },
    {
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "src": "/_fonts/(.*)"
    },
    {
      "src": "/_nuxt/builds/meta(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/builds(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/_fonts(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}